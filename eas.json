{
  "cli": {
    "version": ">= 7.3.0",
    "requireCommit": false,
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true,
        "resourceClass": "medium"
      },
      "android": {
        "gradleCommand": ":app:assembleDebug",
        "env": {
          "EXPO_NDK_VERSION": "26.1.10909125"
        },
        "buildType": "apk",
        "resourceClass": "medium",
        "withoutCredentials": true
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "withoutCredentials": true
      }
    },
    "production": {
      "autoIncrement": true,
      "ios": {
        "resourceClass": "medium",
        "credentialsSource": "local",
        "image": "macos-sequoia-15.3-xcode-16.2",
        "appleTeamId": "2V8LZ2444Y"
      },
      "android": {
        "buildType": "app-bundle",
        "credentialsSource": "remote",
        "gradleCommand": ":app:bundleRelease"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6743064638",
        "appleTeamId": "2V8LZ2444Y"
      },
      "android": {
        "serviceAccountKeyPath": "./pc-api-key.json",
        "track": "production"
      }
    }
  }
}